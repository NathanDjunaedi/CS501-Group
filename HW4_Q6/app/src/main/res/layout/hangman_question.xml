<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <ImageView
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:paddingStart="80dp"
        android:paddingTop="80dp"
        android:layout_width="250dp"
        android:layout_height="150dp"
        android:src="@drawable/limbs"
        android:scaleType="fitXY"
        tools:ignore="RtlSymmetry" />

    <ImageView
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:paddingStart="80dp"
        android:paddingEnd="180dp"
        android:paddingBottom="120dp"
        android:rotation="90"
        android:layout_width="525dp"
        android:layout_height="200dp"
        android:src="@drawable/limbs"
        android:scaleType="fitXY"
        tools:ignore="RtlSymmetry" />

    <ImageView
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:paddingStart="60dp"
        android:paddingBottom="275dp"
        android:layout_width="170dp"
        android:layout_height="350dp"
        android:src="@drawable/limbs"
        android:scaleType="fitXY"
        tools:ignore="RtlSymmetry" />

    <ImageView
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:paddingEnd="240dp"
        android:rotation="90"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:src="@drawable/limbs"
        android:scaleType="fitXY"
        tools:ignore="RtlSymmetry" />

    <ImageView
        android:id="@+id/head1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:paddingBottom="180dp"
        android:layout_width="30dp"
        android:layout_height="match_parent"
        android:src="@drawable/baseline_0_left"
        tools:ignore="RtlSymmetry" />

    <ImageView
        android:id="@+id/body2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:paddingEnd="115dp"
        android:layout_width="175dp"
        android:layout_height="50dp"
        android:rotation="90"
        android:src="@drawable/limbs"
        android:scaleType="fitXY"
        tools:ignore="RtlSymmetry" />

    <ImageView
        android:id="@+id/arm3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:paddingTop="108dp"
        android:paddingEnd="70dp"
        android:layout_width="120dp"
        android:layout_height="500dp"
        android:rotation="135"
        android:src="@drawable/limbs"
        tools:ignore="RtlSymmetry" />

    <ImageView
        android:id="@+id/arm4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:paddingBottom="108dp"
        android:paddingEnd="70dp"
        android:layout_width="120dp"
        android:layout_height="155dp"
        android:rotation="45"
        android:src="@drawable/limbs"
        android:scaleType="fitXY"
        tools:ignore="RtlSymmetry" />

    <ImageView
        android:id="@+id/leg5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:paddingTop="35dp"
        android:paddingEnd="40dp"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:rotation="115"
        android:src="@drawable/limbs"
        android:scaleType="fitXY"
        tools:ignore="RtlSymmetry" />

    <ImageView
        android:id="@+id/leg6"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:paddingBottom="35dp"
        android:paddingEnd="40dp"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:rotation="65"
        android:src="@drawable/limbs"
        android:scaleType="fitXY"
        tools:ignore="RtlSymmetry" />

    <LinearLayout
        android:id="@+id/AnswerBar"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:paddingTop="250dp"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="30dp"
        android:orientation="horizontal">

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>