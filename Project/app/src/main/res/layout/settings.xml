<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:outlineAmbientShadowColor="#9A1212"
    android:outlineSpotShadowColor="#C80C0C">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout7"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:background="@null"

        android:outlineAmbientShadowColor="@null"
        android:outlineProvider="none"

        android:outlineSpotShadowColor="@null"
        android:requiresFadingEdge="none"
        app:layout_constraintBottom_toTopOf="@+id/cancel_button_settings"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout6"
        app:layout_constraintVertical_bias="0.0">

        <TextView
            android:id="@+id/new_vehicle_textview"

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@null"
            android:text="@string/new_vehicle_text"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/car_year_text"

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@null"

            android:text="@string/car_year_text"
            app:layout_constraintBottom_toBottomOf="@+id/car_year_autocomplete_settings"
            app:layout_constraintEnd_toStartOf="@+id/car_year_autocomplete_settings"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/car_year_autocomplete_settings" />

        <TextView
            android:id="@+id/car_make_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:background="@null"
            android:text="@string/car_make_text"
            app:layout_constraintBottom_toBottomOf="@+id/car_make_autocomplete_settings"
            app:layout_constraintEnd_toStartOf="@+id/car_make_autocomplete_settings"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/car_make_autocomplete_settings" />

        <TextView
            android:id="@+id/car_model_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@null"

            android:text="@string/car_model_text"
            app:layout_constraintBottom_toBottomOf="@+id/car_model_autocomplete_settings"
            app:layout_constraintEnd_toStartOf="@+id/car_model_autocomplete_settings"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/car_model_autocomplete_settings" />

        <Button
            android:id="@+id/settings_add_vehicle_button"

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@null"
            android:text="@string/add_vehicle"


            android:textColor="#FFFFFF"
            app:cornerRadius="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0" />

        <AutoCompleteTextView
            android:id="@+id/car_year_autocomplete_settings"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="AutoCompleteTextView"
            app:layout_constraintBottom_toTopOf="@+id/settings_add_vehicle_button"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.777"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/new_vehicle_textview"
            app:layout_constraintVertical_bias="0.075" />

        <AutoCompleteTextView
            android:id="@+id/car_make_autocomplete_settings"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="AutoCompleteTextView"
            app:layout_constraintBottom_toTopOf="@+id/settings_add_vehicle_button"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.777"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/car_year_autocomplete_settings"
            app:layout_constraintVertical_bias="0.026" />

        <AutoCompleteTextView
            android:id="@+id/car_model_autocomplete_settings"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="AutoCompleteTextView"
            app:layout_constraintBottom_toTopOf="@+id/settings_add_vehicle_button"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.777"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/car_make_autocomplete_settings"
            app:layout_constraintVertical_bias="0.0" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout6"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:background="@null"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.366">

        <TextView
            android:id="@+id/edit_cars_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@null"

            android:text="@string/edit_vehicles"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.013" />

        <Spinner
            android:id="@+id/vehicle_select_settings"
            android:layout_width="330dp"
            android:layout_height="wrap_content"
            android:minHeight="48dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.104"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/edit_cars_text" />

        <ImageView
            android:id="@+id/delete_car_settings"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:background="@null"
            android:importantForAccessibility="no"
            app:layout_constraintBottom_toBottomOf="@+id/vehicle_select_settings"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/vehicle_select_settings"
            app:layout_constraintTop_toTopOf="@+id/vehicle_select_settings"
            app:srcCompat="@drawable/red_cross" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout5"
        android:layout_width="match_parent"
        android:layout_height="218dp"
        android:background="@null"

        app:layout_constraintBottom_toTopOf="@+id/constraintLayout6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.476"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <EditText
            android:id="@+id/new_password_edittext"

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:gravity="center"
            android:hint="@string/new_password"
            android:importantForAutofill="no"
            android:inputType="textPassword"
            android:minHeight="48dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/old_password_edittext"
            app:layout_constraintVertical_bias="0.0"
            tools:layout_editor_absoluteX="87dp" />

        <EditText
            android:id="@+id/old_password_edittext"

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:gravity="center"
            android:hint="@string/old_password"
            android:importantForAutofill="no"
            android:inputType="textPassword"
            android:minHeight="48dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/change_password_text"
            app:layout_constraintVertical_bias="0.076"
            tools:layout_editor_absoluteX="0dp" />

        <TextView
            android:id="@+id/change_password_text"

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@null"
            android:text="@string/change_password"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <Button
            android:id="@+id/change_password_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/set_password"
            android:textColor="#FFFFFF"

            app:cornerRadius="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/new_password_edittext" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button
        android:id="@+id/cancel_button_settings"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@null"
        android:text="@string/back"
        android:textColor="#FFFFFF"

        app:cornerRadius="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout7"
        app:layout_constraintVertical_bias="0.0" />


</androidx.constraintlayout.widget.ConstraintLayout>